<!DOCTYPE html>
<html>
  <head>
    <title>Adventure</title>

  </head>

  <body>
    <h1>All Locations</h1>
    <table>
      <thead>
        <tr>
          <td>Location Name</td>
        </tr>
      </thead>
      <tbody>
        <% @locations.each do |location| %>
          <tr>
          <td> <%= location.name %> </td>
          <td> <%= location.latitude %> </td>
          <td> <%= location.longitude %> </td>
          <td> <%= location.rating %> </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </body>
</html>
