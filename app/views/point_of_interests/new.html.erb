<h1>New Point of Interest</h1>


<%= form_for(@point_of_interest) do |f| %>

	<%= f.label(:name) %>
	<%= f.text_field(:name) %>

	<%= f.label(:longitude) %>
	<%= f.text_field(:longitude) %>

	<%= f.label(:latitude) %>
	<%= f.text_field(:latitude) %>
	<br />
	<%= f.label(:description) %>
	<%= text_area("description") %>
	<br />
	<%= f.submit %>
<% end %>